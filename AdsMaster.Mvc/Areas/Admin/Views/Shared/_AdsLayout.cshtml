@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section HeaderContent {
    @RenderSection("HeaderContent", required: false)
    <environment include="Development">
        <link rel="shortcut icon" href="~/favicon.ico">
        <script src="~/lib/signalr/dist/signalr.js"></script>
        <script src="~/lib/PopForums/src/PopForums.js"></script>

        <!-- Favicon -->
        <!-- Bootstrap CSS -->
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" type="text/css">
        <!-- Font Awesome CSS -->
        <link rel="stylesheet" href="~/Classifieds/dist/fonts/all.min.css" type="text/css">
        <!-- Line Icons CSS -->
        <link rel="stylesheet" href="~/Classifieds/dist/fonts/line-icons/line-icons.css" type="text/css">
        <!-- Animate CSS -->
        <link rel="stylesheet" href="~/Classifieds/dist/extras/animate.css" type="text/css">
        <!-- Owl Carousel -->
        <link rel="stylesheet" href="~/Classifieds/dist/extras/owl.carousel.css" type="text/css">
        <link rel="stylesheet" href="~/Classifieds/dist/extras/owl.theme.css" type="text/css">
        <!-- Slicknav js -->
        <link rel="stylesheet" href="~/lib/slicknav/dist/slicknav.min.css" type="text/css">
        <!-- Main Styles -->
        <link rel="stylesheet" href="~/Classifieds/dist/css/main.css" type="text/css">
        <!-- Responsive CSS Styles -->
        <link rel="stylesheet" href="~/Classifieds/dist/css/responsive.css" type="text/css">
    </environment>

    <environment exclude="Development">
        <script src="~/lib/signalr/dist/signalr.min.js"></script>
        <script src="~/lib/PopForums/dist/PopForums.min.js"></script>

        <!-- Favicon -->
        <link rel="shortcut icon" href="~/favicon.ico">
        <!-- Bootstrap CSS -->
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" type="text/css">
        <!-- Font Awesome CSS -->
        <link rel="stylesheet" href="~/Classifieds/dist/fonts/all.min.css" type="text/css">
        <!-- Line Icons CSS -->
        <link rel="stylesheet" href="~/Classifieds/dist/fonts/line-icons/line-icons.css" type="text/css">
        <!-- Animate CSS -->
        <link rel="stylesheet" href="~/Classifieds/dist/extras/animate.css" type="text/css">
        <!-- Owl Carousel -->
        <link rel="stylesheet" href="~/Classifieds/dist/extras/owl.carousel.css" type="text/css">
        <link rel="stylesheet" href="~/Classifieds/dist/extras/owl.theme.css" type="text/css">
        <!-- Slicknav js -->
        <link rel="stylesheet" href="~/lib/slicknav/dist/slicknav.min.css" type="text/css">
        <!-- Main Styles -->
        <link rel="stylesheet" href="~/Classifieds/dist/css/main.css" type="text/css">
        <!-- Responsive CSS Styles -->
        <link rel="stylesheet" href="~/Classifieds/dist/css/responsive.css" type="text/css">
    </environment>
}

@await Component.InvokeAsync("UserNav")

@RenderBody()

@*Footer Section Start*@
<footer>
    @*Footer Area Start*@
    <section class="footer-Content">
        <div class="container">
            <div class="row">
                <div class="col-lg-3 col-md-6 col-xs-12 wow fadeIn" data-wow-delay="0.2s">
                    <div class="widget">
                        <h3 class="block-title">Admin Page</h3>
                        <div class="textwidget">
                            <p>Page for administrator only</p>
                        </div>
                    </div>
                </div>
           </div>
        </div>
    </section>
    @*Footer area End*@
    @*Copyright Start*@
    <div id="copyright">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <div class="site-info float-left">
                        <p>© 2020 <a href="http://dubastic.com/" rel="nofollow">DUBASTIC</a></p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    @*Copyright End*@

</footer>
@*Footer Section End*@

@*Go To Top Link*@
<a href="#" class="back-to-top">
    <i class="fas fa-angle-up"></i>
</a>

@*Main JS*@

