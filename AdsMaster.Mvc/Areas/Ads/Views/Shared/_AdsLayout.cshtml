	<meta name="viewport" content="width=device-width, initial-scale=1">
	<meta http-equiv="content-type" content="text/html; charset=utf-8">
	<meta name="author" content="Ads Master">
	<title>@ViewBag.Title</title>

	<!-- Favicon -->
	<link rel="shortcut icon" href="~/favicon.ico">
	<!-- Bootstrap CSS -->
	<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" type="text/css">
	<!-- Font Awesome CSS -->
	<link rel="stylesheet" href="~/Classifieds/dist/fonts/all.min.css" type="text/css">
	<!-- Line Icons CSS -->
	<link rel="stylesheet" href="~/Classifieds/dist/fonts/line-icons/line-icons.css" type="text/css">
	<!-- Animate CSS -->
