									<li><a class="dropdown-item" href="@Url.Action("Signup", "Account")"><i class="lnr lnr-user"></i> Signup</a></li>
									<li><a class="dropdown-item" href="@Url.Action("ForgotPassword", "Account")"><i class="lnr lnr-sync"></i> Forgot Password</a></li>
									<li><a class="dropdown-item" href="@Url.Action("CloseAccount", "Account")"><i class="lnr lnr-cross"></i>Account close</a></li>
								</ul>
							</li>
							<li class="postadd">
								<a class="btn btn-common btn-post" href="@Url.Action("New", "Post")"><span class="fas fa-plus-circle"></span> Post an Ad</a>
							</li>
						</ul>
					</div>
				</div>
			</div>
			<div class="mobile-menu" data-logo="/Classifieds/dist/img/logo-mobile.png"></div>
