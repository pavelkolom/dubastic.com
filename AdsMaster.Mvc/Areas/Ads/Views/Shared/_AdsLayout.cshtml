	<!-- Main Styles -->
	<link rel="stylesheet" href="~/Classifieds/dist/css/main.css" type="text/css">
	<!-- Responsive CSS Styles -->
	<link rel="stylesheet" href="~/Classifieds/dist/css/responsive.css" type="text/css">

</head>

<body>
