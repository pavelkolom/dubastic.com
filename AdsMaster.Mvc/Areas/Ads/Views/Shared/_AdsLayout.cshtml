									<li><a class="dropdown-item" href="@Url.Action("Details", "Post")">Ads Details</a></li>
									<li><a class="dropdown-item" href="@Url.Action("New", "Post")">Ads Post</a></li>
									<li><a class="dropdown-item" href="@Url.Action("PostNotFound", "Post")">404</a></li>
									<li><a class="dropdown-item" href="@Url.Action("FAQ", "About")">Faq</a></li>
								</ul>
							</li>
