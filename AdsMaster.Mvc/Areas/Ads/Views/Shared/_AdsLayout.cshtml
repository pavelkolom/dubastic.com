									<li><a class="dropdown-item" href="@Url.Action("Details", "Blog")">Blog Details</a></li>
								</ul>
							</li>
							<li class="nav-item">
								<a class="nav-link" href="@Url.Action("Contact", "About")">
									Contact
								</a>
							</li>
